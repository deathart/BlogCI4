{
	"description": "Blog write with Codeigniter v4dev",
	"name": "deathart/blogci4",
	"type": "project",
	"homepage": "https://github.com/deathart/BlogCI4",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/deathart/BlogCI4/issues",
		"source": "https://github.com/deathart/BlogCI4"
	},
	"autoload": {
		"psr-4": {
			"CodeIgniter\\": "system/"
		}
	},
	"require": {
		"php": ">=7.1",
		"zendframework/zend-escaper": "^2.5",
		"paragonie/sodium_compat": "^1.6",
		"kint-php/kint": "^2.2",
		"twig/twig": "^2.4"
	},
	"require-dev": {
		"friendsofphp/php-cs-fixer": "^2.9"
	},
	"scripts": {
		"post-update-cmd": [
			"composer dump-autoload",
			"CodeIgniter\\ComposerScripts::postUpdate"
		]
	},
	"authors": [
		{
			"name": "deathart",
			"email": "contact@deathart.fr",
			"homepage": "https://deathart.fr"
		}
	]
}
